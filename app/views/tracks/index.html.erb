<div class="container">
  <h1>Songs played by <%= ENV['USERNAME'] %></h1>
</div>
<a href="/tracks/fetch_new_tracks" class="button">Fetch New Tracks</a>
<table class="table">
  <tr>
    <th>Name</th>
    <th>Artist</th>
    <th>Album</th>
    <th>Listened At</th>
  </tr>
  <% @tracks.each do |track| %>
    <tr>
      <td><%= track.name %></td>
      <td><%= track.artist %></td>
      <td><%= track.album %></td>
      <td><%= track.listened_at %></td>
    </tr>
  <% end %>
</table>
